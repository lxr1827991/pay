
  `id` int(11) NOT NULL,
  `apptype` int(2) NOT NULL COMMENT '1:android,2:ios',
  `file` varchar(200) COLLATE utf8_bin NOT NULL COMMENT '路径',
  `version_code` varchar(20) COLLATE utf8_bin NOT NULL,
  `info` varchar(100) COLLATE utf8_bin DEFAULT NULL,
  `addtime` bigint(20) NOT NULL,
  `createtime` bigint(20) DEFAULT NULL,
  `updatetime` bigint(20) DEFAULT NULL,
  `remark` varchar(50) COLLATE utf8_bin DEFAULT NULL,
  `is_valid` int(2) NOT NULL DEFAULT '1',
  