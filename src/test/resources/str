 `id` varchar(32) NOT NULL,
  `orderid` varchar(32) NOT NULL DEFAULT '' COMMENT '订单编号',
  `ordercode` varchar(32) NOT NULL DEFAULT '' COMMENT '订单编号',
  `addtime` bigint(17) NOT NULL DEFAULT '0' COMMENT '订单生成时间',
  `applytime` bigint(17) NOT NULL DEFAULT '0' COMMENT '申请时间',
  `applyduration` bigint(17) NOT NULL DEFAULT '0' COMMENT '申请时长',
  `starttime` bigint(17) NOT NULL DEFAULT '0' COMMENT '车辆驶入时间',
  `billstart` bigint(17) NOT NULL DEFAULT '0' COMMENT '计费开始时间',
  `mebid` varchar(32) NOT NULL DEFAULT '' COMMENT '订单所属人id',
  `paymebid` varchar(32) NOT NULL DEFAULT '' COMMENT '订单代缴人id',
  `mebtel` varchar(15) NOT NULL DEFAULT '' COMMENT '客户电话',
  `comid` varchar(32) NOT NULL DEFAULT '' COMMENT '业主ID',
  `state` tinyint(2) NOT NULL DEFAULT '1' COMMENT '订单状态(1-进行中，2-已完成，3-待补缴)',
  `substitutestate` tinyint(2) NOT NULL DEFAULT '1' COMMENT '代缴状态(1-未完成，2-已完成)',
  `applyway` tinyint(2) NOT NULL DEFAULT '0' COMMENT '申请方式',
  `berthcode` varchar(12) NOT NULL DEFAULT '' COMMENT '泊位号',
  `sensor` tinyint(2) NOT NULL DEFAULT '0' COMMENT '是否以车检器时间为准  0-车检器信息无效 1-以车检器时间结束',
  `carplate` varchar(12) NOT NULL DEFAULT '' COMMENT '车牌号',
  `cartype` tinyint(2) NOT NULL DEFAULT '0',
  `rfid` varchar(13) NOT NULL DEFAULT '' COMMENT 'RFID',
  `poscode` varchar(32) NOT NULL DEFAULT '' COMMENT 'pos机编号',
  `userid` varchar(32) NOT NULL DEFAULT '' COMMENT 'pos机使用者ID',
  `clientid` varchar(32) NOT NULL DEFAULT '' COMMENT '客户端ID',
  `psamcode` varchar(32) NOT NULL DEFAULT '' COMMENT 'psam卡ID',
  `paymethod` tinyint(2) NOT NULL DEFAULT '0' COMMENT '付款方式(1-后付费  2-预付费)',
  `areaid` varchar(32) NOT NULL DEFAULT '' COMMENT '片区ID',
  `cantonid` varchar(32) NOT NULL DEFAULT '' COMMENT '行政区ID',
  `sectionid` varchar(32) NOT NULL DEFAULT '' COMMENT '路段ID',
  `shouldpay` decimal(11,2) NOT NULL DEFAULT '0.00',
  `actualpay` decimal(11,2) NOT NULL DEFAULT '0.00',
  `creator` varchar(10) DEFAULT NULL COMMENT '创建者',
  `createtime` bigint(20) DEFAULT NULL COMMENT '创建时间',
  `updatetime` bigint(20) DEFAULT NULL COMMENT '版本时间戳',
  `remark` varchar(200) DEFAULT NULL COMMENT '系统备注',
  `isvalid` int(2) DEFAULT '1' COMMENT '是否有效 0-无效 1-有效',