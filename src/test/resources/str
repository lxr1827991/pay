
  `id` varchar(32) NOT NULL COMMENT '主键',
  `meb_id` varchar(32) NOT NULL COMMENT '关联会员信息表ID',
  `type` tinyint(2) NOT NULL COMMENT '充值类型 1-表示充值 2-代表支付 3-代表退款',
  `recharge_way` tinyint(2) NOT NULL COMMENT '充值途径 1-表示APP 2-微信公众号',
  `pay_way` tinyint(2) DEFAULT NULL COMMENT '付款方式 1-余额 2-优惠券 3-停车卡 4-现金 5-停车劵 6-赠送金额 7-免费',
  `money` decimal(10,2) NOT NULL COMMENT '充值金额',
  `befor_money` decimal(10,2) NOT NULL COMMENT '充值前金额',
  `after_money` decimal(10,2) NOT NULL COMMENT '充值后金额',
  `status` tinyint(2) NOT NULL DEFAULT '0' COMMENT '充值状态 0-充值中 1-充值成功 2-充值失败 3-取消支付',
  `bank_type` tinyint(2) DEFAULT NULL COMMENT '充值银行 1-支付宝 2-微信支付 3-某银行 4-洪城一卡通',
  `bank_card` varchar(30) DEFAULT NULL COMMENT '充值银行卡号',
  `trade_water` varchar(64) DEFAULT NULL COMMENT '第三方交易流水',
  `card_order_id` varchar(32) DEFAULT NULL COMMENT '停车卡续费订单id',
  `park_order_detail_id` varchar(32) DEFAULT NULL COMMENT '停车订单明细id',
  `create_time` varchar(16) NOT NULL COMMENT '创建时间',
  `update_time` varchar(16) NOT NULL COMMENT '版本时间戳',
  `is_valid` tinyint(2) NOT NULL COMMENT '是否有效 0-无效 1-有效',
  